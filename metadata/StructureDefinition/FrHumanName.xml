        <strength value="extensible" />
        <description value="Civilités d'exercice d'un professionnel du RASS" />
        <valueSet value="https://mos.esante.gouv.fr/NOS/JDV_J79-CiviliteExercice-RASS/FHIR/JDV-J79-CiviliteExercice-RASS" />
      </binding>
    </element>
